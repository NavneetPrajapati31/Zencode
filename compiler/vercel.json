{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**/*"],
        "nodeVersion": "22.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/compiler",
      "dest": "/index.js"
    },
    {
      "src": "/ai-review",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "true"
  }
}
